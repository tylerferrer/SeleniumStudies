	    public static void main(String[] args) {
	    	String caminhoDriver = "\\IdeaProjects\\seleniumStudies\\Amazon\\src\\main\\resources\\drivers\\chromedriver-win64\\";
	        DriverFactory.getDriver().get("file:///" + System.getProperty("user.dir") + caminhoDriver + "chromedriver.exe");
	        

	        // Abra o Google
	        

	        // Encerre a sess√£o do WebDriver
	        DriverFactory.killDriver();
	}
}
