package pages;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

import utils.DriverFactory;


public class AmazonPage {
		
	    public static void main(String[] args) {
	    	String caminhoDriver = "C:\\Users\\Tyler\\IdeaProjects\\seleniumStudies\\Amazon\\src\\main\\resources\\drivers\\chromedriver-win64\\";
	        DriverFactory.getDriver().get(System.getProperty(caminhoDriver + "chromedriver.exe"));
	        

	        // Crie uma instância do WebDriver para o Chrome
	        WebDriver driver = new ChromeDriver();

	        // Abra o Google
	        driver.get("https://www.google.com");

	        // Encerre a sessão do WebDriver
	        driver.quit();
	}
}
